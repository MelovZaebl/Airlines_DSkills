<Application x:Class="_17_10_2022.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:_17_10_2022"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="TeXGyreAdventor">pack://application:,,,/Fonts/#TeXGyreAdventor</FontFamily>
        <FontFamily x:Key="verdana">pack://application:,,,/Fonts/#verdana</FontFamily>
        <FontFamily x:Key="TC">pack://application:,,,/Fonts/#TC</FontFamily>
        <Style TargetType="{x:Type Window}">
            <Setter Property="FontFamily" Value="TeX Gyre Adventor" />
        </Style>
    </Application.Resources>
</Application>
